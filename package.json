{
	"name": "binarytf",
	"version": "2.0.0",
	"description": "Binary Term Format",
	"author": "kyranet <kyradiscord@gmail.com>",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"browser": "dist/index.umd.js",
	"unpkg": "dist/index.umd.js",
	"types": "dist/index.d.ts",
	"homepage": "https://binarytf.github.io/binarytf",
	"private": false,
	"files": [
		"dist"
	],
	"scripts": {
		"docs": "typedoc",
		"lint": "eslint --fix src tests --ext ts",
		"format": "prettier --write {src,tests}/**/*.ts",
		"format:check": "prettier --check {src,tests}/**/*.ts",
		"test": "jest",
		"test:watch": "jest --watch",
		"prepublishOnly": "yarn build",
		"update": "yarn upgrade-interactive --latest",
		"build": "rollup -c rollup.config.ts",
		"watch": "yarn build -w",
		"sversion": "standard-version",
		"commit": "git-cz",
		"cz": "git-cz"
	},
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@skyra/eslint-config": "^5.3.3",
		"@types/jest": "^26.0.14",
		"@types/node": "^14.11.10",
		"@typescript-eslint/eslint-plugin": "^4.4.1",
		"@typescript-eslint/parser": "^4.4.1",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^7.11.0",
		"eslint-config-prettier": "^6.13.0",
		"eslint-plugin-prettier": "^3.1.4",
		"husky": "^4.3.0",
		"jest": "^26.5.3",
		"jest-circus": "^26.5.3",
		"lint-staged": "^10.4.2",
		"prettier": "^2.1.2",
		"pretty-quick": "^3.1.0",
		"rollup": "^2.32.0",
		"rollup-plugin-cleaner": "^1.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.28.0",
		"standard-version": "^9.0.0",
		"ts-jest": "^26.4.1",
		"ts-node": "^9.0.0",
		"typedoc": "^0.19.2",
		"typedoc-plugin-nojekyll": "^1.0.1",
		"typescript": "^4.0.3"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/binarytf/binarytf.git"
	},
	"engines": {
		"node": ">=12",
		"npm": ">=6"
	},
	"keywords": [
		"binary",
		"term",
		"format",
		"serializer",
		"deserializer",
		"circular"
	],
	"bugs": {
		"url": "https://github.com/binarytf/binarytf/issues"
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "yarn pretty-quick --staged && yarn lint-staged"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"resolutions": {
		"acorn": "^7.3.1",
		"apollo-codegen": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz",
		"chalk": "^4.1.0",
		"jest-environment-jsdom": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz",
		"jest-jasmine2": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz",
		"kind-of": "^6.0.3",
		"lodash": "^4.17.20",
		"minimist": "^1.2.5"
	}
}
