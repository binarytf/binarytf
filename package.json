{
  "name": "binarytf",
  "version": "2.0.0",
  "description": "Binary Term Format",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build:browser": "webpack",
    "lint": "eslint --ext ts src --fix",
    "test:lint": "eslint --ext ts src",
    "test": "yarn build && node dist/test/suite.js",
    "test:azure": "yarn build && node dist/test/suite.js | tap-xunit --package=\"binarytf\" > test.xml",
    "coverage": "yarn nyc --require ts-node/register --require source-map-support/register tape src/test/suite.ts | yarn tap-nyc",
    "coverage:report": "yarn nyc report --reporter=cobertura",
    "build": "tsc -p .",
    "watch": "tsc -p . -w"
  },
  "keywords": [
    "binary",
    "term",
    "format",
    "serializer",
    "deserializer",
    "circular"
  ],
  "repository": "https://github.com/kyranet/binarytf",
  "author": "Antonio Rom√°n <kyradiscord@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/node": "^14.0.27",
    "@types/tape": "^4.13.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.6.0",
    "eslint-config-bamboo": "^5.1.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "tap-nyc": "^1.0.3",
    "tap-xunit": "^2.4.1",
    "tape": "^5.0.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
