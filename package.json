{
  "name": "binarytf",
  "version": "2.0.0",
  "description": "Binary Term Format",
  "author": "Antonio Rom√°n <kyradiscord@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "https://github.com/kyranet/binarytf",
  "private": false,
  "scripts": {
    "prepublishOnly": "yarn build",
    "lint": "eslint --ext ts src --fix",
    "test:lint": "eslint --ext ts src",
    "build": "tsc -p .",
    "watch": "tsc -p . -w"
  },
  "files": [
		"dist"
	],
  "keywords": [
    "binary",
    "term",
    "format",
    "encode",
    "decode",
    "serializer",
    "deserializer",
    "encoder",
    "decoder",
    "circular"
  ],
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@skyra/eslint-config": "^5.3.0",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.6.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "standard-version": "^8.0.2",
    "typescript": "^3.9.7"
  },
  "engines": {
		"node": ">=12",
		"npm": ">=6"
  },
  "husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "yarn lint-staged"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.ts": [
			"eslint --fix --ext ts"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	}
}
